{
  "name": "booking-backend",
  "version": "1.0.0",
  "description": "Backend API for property booking system with Stripe and Paystack payments",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node --no-deprecation -r tsconfig-paths/register dist/app.js",
    "dev": "tsx watch --no-warnings src/app.ts",
    "dev:debug": "tsx watch --no-warnings --inspect src/app.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**/*.ts\" --ignore-pattern \"**/*.test.ts\" --ignore-pattern \"**/*.spec.ts\"",
    "lint:critical": "eslint src/services/pricingEngine.ts src/services/financeConfig.ts src/services/bookingAccessControl.ts src/services/paymentFinalization.ts --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.ts\" --fix --ignore-pattern \"**/*.test.ts\" --ignore-pattern \"**/*.spec.ts\"",
    "cron:complete-bookings": "tsx src/jobs/runBookingCompletion.ts"
  },
  "keywords": [
    "booking",
    "property",
    "stripe",
    "paystack",
    "prisma",
    "typescript",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.3",
    "helmet": "^7.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "paystack": "^2.0.1",
    "resend": "^6.6.0",
    "socket.io": "^4.8.1",
    "stripe": "^13.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.11.0",
    "pdfkit": "^0.17.2",
    "@types/express": "^4.17.20",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.29",
    "@types/pdfkit": "^0.17.4",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.15",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.19",
    "@types/swagger-jsdoc": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prisma": "^6.16.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^3.14.0"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
